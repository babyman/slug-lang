
//
// simple var declaration
// ----------------------

var x = 10;

x.assertEqual(10);

var x = x + 1;

x.assertEqual(11);


//
// var declaration with destructured lists
// ---------------------------------------

var f1 = fn(a, b) {
    [a, b]
}

var [a, b] = f1(1, 2);

a.assertEqual(1);

b.assertEqual(2);

