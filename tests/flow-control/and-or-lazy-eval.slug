var {*} = import(
    "slug.test"
);

var callCount = 0;

val f = fn(b) {
    callCount = callCount + 1;
    b
}

if(f(false) && f(true)) {
    // empty
}

callCount /> assertEqual(1);

// one more AND check
callCount = 0;

if(f(true) && f(false)) {
    // empty
}

callCount /> assertEqual(2);


//
// OR optimizations
//
callCount = 0;

if(f(true) || f(false)) {
    // empty
}

callCount /> assertEqual(1);

// one more OR check
callCount = 0;

if(f(false) || f(true)) {
    // empty
}

callCount /> assertEqual(2);
