var {*} = import(
    "slug.test"
);

//
// Fibonacci sequence calculation using tail call recursion
//

var fib = fn(n, a = 0, b = 1) {
    if (n == 0) {
        a;
    } else if (n == 1) {
        b;
    } else {
        fib(n - 1, b, a + b);
    }
}

fib(10) /> assertEqual(55, "Fibonacci 10 equals 55");
